			<position>LL</position>
			<priority>5</priority>
			<shadowColor red="244" green="43" blue="22"/>
			<shadowSize x="15" y="14"/>
			<size>SMALL</size>
                        <Style>
                          <geomTransform>VERTICES</geomTransform>
                          <initialGap>20.5</initialGap>
                          <polarOffset>[uv_length_2] [uv_angle]</polarOffset>
                        </Style>
                        <Style>
                          <initialGap>2</initialGap>
                        </Style>
			<wrap>\n</wrap>
		</Label>
		<outlineColor red="12" green="45" blue="12"/>
