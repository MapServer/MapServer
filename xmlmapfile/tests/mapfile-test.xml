			<align>CENTER</align>
			<angle>FOLLOW</angle>
			<antialias>TRUE</antialias>
			<buffer>2</buffer>
			<colorAttribute>[myColor]</colorAttribute>
			<encoding>ISO 8859-1</encoding>
			<font>serif</font>
			<force>TRUE</force>
			<maxLength>244</maxLength>
			<maxOverlapAngle>30</maxOverlapAngle>
			<maxSize>244</maxSize>
			<minDistance>24</minDistance>
