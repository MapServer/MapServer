<se:Fill>
<se:SvgParameter name="fill">#0000ff</se:SvgParameter>
</se:Fill>
<se:Stroke>
<se:SvgParameter name="stroke">#ffff00</se:SvgParameter>
<se:SvgParameter name="stroke-width">5.00</se:SvgParameter>
</se:Stroke>
</se:PolygonSymbolizer>
</se:Rule>
<se:Rule>
<se:Name>red</se:Name>
<ogc:Filter><ogc:PropertyIsEqualTo><ogc:PropertyName>NAME</ogc:PropertyName><ogc:Literal>Bretagne </ogc:Literal></ogc:PropertyIsEqualTo></ogc:Filter>
