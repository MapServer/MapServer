      <expires>1</expires>
      <!--<auto_expire>86400</auto_expire>-->
      <metabuffer>10</metabuffer>
   </tileset>

   <!--
   <tileset name="bluemarble">
      <source>bluemarble</source>
      <cache>disk</cache>
      <metatile>5 5</metatile>
      <format>PNG</format>
      <grid>google,WGS84</grid>
   </tileset>
