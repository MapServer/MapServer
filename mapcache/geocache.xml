      <grid>g</grid>
   </tileset>
   <tileset name="osm">
      <metadata>
         <title>osm mapserver served map of midi-pyrénées</title>
         <abstract>see http://mapserver-utils.googlecode.com</abstract>
      </metadata>
      <source>osm</source>
      <cache>disk</cache>
